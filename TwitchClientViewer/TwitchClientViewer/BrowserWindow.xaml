<Window x:Class="TwitchClientViewer.BrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwitchClientViewer"
        mc:Ignorable="d"
        WindowStyle="ToolWindow"
        Title="BrowserWindow" Height="800" Width="400">
    <Grid>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <WebBrowser x:Name="webBrowser" Margin="0,16,0,0" Navigated="webBrowser_Navigated" Navigating="webBrowser_Navigating"/>
        <Button x:Name="button" Content="Press to close please.Thanks" Margin="10,21,10,10" Click="button_Click"/>

    </Grid>
</Window>